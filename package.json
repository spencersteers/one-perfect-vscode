{
    "name": "one-perfect-vscode",
    "description": "VSCode made Perfect",
    "publisher": "spencersteers",
    "version": "0.0.10",
    "scripts": {
        "install-extension": "vsce package && find -s ./*.vsix | tail -n 1 | xargs code --install-extension"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/spencersteers/one-perfect-vscode"
    },
    "bugs": {
        "url": "https://github.com/spencersteers/one-perfect-vscode/issues"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "One Perfect",
                "uiTheme": "vs-dark",
                "path": "./themes/one-perfect.json"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^2.11.0"
    }
}
